    <groupId>eu.unifiedviews</groupId> 
    <artifactId>extractor_psp_cz_metadata</artifactId>
	<name>ext. psp cz metadata</name>
    <packaging>bundle</packaging>
    <version>1.0-SNAPSHOT</version>
