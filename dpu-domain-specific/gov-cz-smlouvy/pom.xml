<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>cz.cuni.mff.xrg.uv</groupId>
        <artifactId>unifiedviews-dpu-base</artifactId>
        <version>2.0.0</version>
    </parent>    
    <artifactId>UK-gov_cz_smlouvy</artifactId>
    <name>ext. gov cz smlouvy</name>
    <packaging>bundle</packaging>
    <version>2.1.1</version>
    <description>This extractor scrapes Smlouvy from portal.gov.cz</description>

    <properties>    
        <dpu.package>cz.opendata.linked.cz.gov.smlouvy</dpu.package>
        <dpu.mainClass>Extractor</dpu.mainClass>
        <!-- Import t-Xslt packages as optional. -->
        <!--<osgi.packages>cz.cuni.mff.xrg.uv.transformer.xslt.*;resolution:="optional",</osgi.packages>-->
    </properties>

    <dependencies>
        <dependency>
            <groupId>commons-io</groupId>
            <artifactId>commons-io</artifactId>
            <version>2.4</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>cz.cuni.mff.xrg.uv</groupId>
            <artifactId>boost-dpu</artifactId>
            <version>[${uk.version.min},${uk.version.max})</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>cz.cuni.mff.xrg.uv</groupId>
            <artifactId>utils-dataunit</artifactId>
            <version>[${uk.version.min},${uk.version.max})</version>
            <scope>compile</scope>
        </dependency>
        <!-- Dependency on other DPUs -->
        <dependency>
            <groupId>cz.cuni.mff.xrg.uv</groupId>
            <artifactId>t-uk-xslt</artifactId>
            <version>1.0.0</version>
            <scope>provided</scope>
            <optional>true</optional>
        </dependency>
    </dependencies>
</project>

